#!/bin/bash

####
# Begin
####
old_dir=`pwd`
cd "$(dirname "$0")"

echo -n "Loading 'andrealexandre/bash-dev-tools'... ";
ts=$(perl -MTime::HiRes -e 'printf("%.0f\n",Time::HiRes::time()*1000)');

####
# Imports
####
source tools/git_includes;

####
# End
####
tf=$(perl -MTime::HiRes -e 'printf("%.0f\n",Time::HiRes::time()*1000)');
echo "in '$((tf - ts))' ms";

cd $old_dir